{
	"name": "Events Logger",
	"manifest_version": 2,
	"version": "1.0",
	"background": {
		"scripts": ["js/background.js"]
	},
	"icons": {
		"32": "icons/icon_32.png"
	},
	"browser_action": {
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["js/content_script.js"],
			"match_about_blank": true,
			"all_frames": true
		}
	],
	"permissions": [
		"<all_urls>",
		"downloads",
		"storage",
		"unlimitedStorage"
	]
}